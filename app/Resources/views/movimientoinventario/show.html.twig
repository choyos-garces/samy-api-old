{% extends 'base.html.twig' %}

{% block body %}
    <h1>MovimientoInventario</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ movimientoInventario.id }}</td>
            </tr>
            <tr>
                <th>Tipomovimiento</th>
                <td>{{ movimientoInventario.tipoMovimiento }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if movimientoInventario.fecha %}{{ movimientoInventario.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('movimientoinventario_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('movimientoinventario_edit', { 'id': movimientoInventario.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
